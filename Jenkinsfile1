pipeline
{
	agent any
		stages
		{
			stage ("fetch")
			{
				steps
				{
					echo 'fetching code'
				}
			}
			stage ("build")
			{
				steps
				{
					echo 'building code'
				}
			}
			stage ("test")
			{
				parallel
				{
					stage ("test A")
					{
						steps
						{
							echo 'testing code A'
							sleep 30
						}
					}
					stage ("test B")
					{
						steps
						{
							echo 'testing code B'
							sleep 40
						}
					}
					stage ("test C")
					{
						steps
						{
							echo 'testing code C'
							sleep 50
						}
					}
					stage ("test D")
					{
						steps
						{
							echo 'testing code D'
						}
					}
				}
			}
		}
}
